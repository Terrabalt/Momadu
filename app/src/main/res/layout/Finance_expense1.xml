<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.slideshow.SlideshowFragment">


    <EditText
        android:id="@+id/editTextNumberDecimal"
        android:layout_width="369dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="148dp"
        android:autofillHints="currency"
        android:ems="10"
        android:hint="@string/finance_name"
        android:inputType="numberDecimal"
        app:layout_constraintEnd_toStartOf="@+id/button"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="205dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="20dp"
        android:layout_height="33dp"
        android:src="@drawable/pen"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="228dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="347dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="#C4C4C4"
        android:text="@string/note_name"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="223dp" />

    <com.google.android.material.textfield.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="249dp"
        tools:layout_editor_absoluteY="281dp"/>

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="286dp"
        android:layout_height="51dp"
        android:background="#00FFFFFF"
        android:src="@drawable/recatngle"
        tools:layout_editor_absoluteX="71dp"
        tools:layout_editor_absoluteY="621dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Choose Category"
        android:textColor="#FFFFFF"
        android:textSize="70px"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="628dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="204dp"
        android:layout_height="28dp"
        android:text="Monday, 22 Maret 2021"
        android:textSize="45px"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="50dp" />

</androidx.constraintlayout.widget.ConstraintLayout>