<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sti.ppam.momadu">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Momadu">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Momadu.NoActionBar"/>
        <activity
            android:name=".FirstPageActivity"
            android:parentActivityName=".MainActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity
            android:name=".ui.slideshow.FinanceExpense"
            android:label="@string/finance_expense_name"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.slideshow.FinanceIncome"
            android:label="@string/finance_income_name"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>